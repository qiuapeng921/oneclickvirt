auth:
    enable-email: false
    enable-public-registration: true
    enable-qq: false
    enable-telegram: false
captcha:
    enabled: true
    expire-time: 300
    height: 40
    length: 4
    width: 120
cdn:
    base-endpoint: https://cdn.spiritlhl.net/
    endpoints:
        - https://cdn0.spiritlhl.top/
        - http://cdn3.spiritlhl.net/
        - http://cdn1.spiritlhl.net/
        - http://cdn2.spiritlhl.net/
cors:
    mode: ""
    whitelist: []
invite-code:
    enabled: false
    required: false
inviteCode:
    enabled: true
    required: true
jwt:
    buffer-time: 1d
    expires-time: 7d
    issuer: oneclickvirt
    signing-key: ""
mysql:
    auto-create: true
    config: charset=utf8mb4&parseTime=True&loc=Local
    db-name: oneclickvirt
    engine: InnoDB
    log-mode: error
    log-zap: false
    max-idle-conns: 10
    max-lifetime: 3600
    max-open-conns: 100
    password: ""
    path: 127.0.0.1
    port: "3306"
    prefix: ""
    singular: false
    username: root
quota:
    default-level: 1
    level-limits:
        "1":
            max-instances: 1
            max-resources:
                bandwidth: 10
                cpu: 1
                disk: 13020
                memory: 1024
            max-traffic: 1024
redis:
    addr: ""
    db: 0
    password: ""
sysm:
    db-type: mysql
system:
    addr: 8888
    db-type: mysql
    env: development
    frontend-url: ""
    iplimit-count: 15000
    iplimit-time: 3600
    oss-type: local
    use-multipoint: false
    use-redis: false
task:
    delete-retry-count: 3
    delete-retry-delay: 2
upload:
    max-avatar-size: 2
zap:
    compress-logs: true
    director: storage/logs
    encode-level: LowercaseLevelEncoder
    format: console
    level: debug
    log-in-console: false
    max-array-elements: 5
    max-backups: 15
    max-file-size: 5
    max-log-length: 2000
    max-string-length: 1000
    prefix: '[oneclickvirt]'
    retention-day: 3
    show-line: false
    stacktrace-key: stacktrace
